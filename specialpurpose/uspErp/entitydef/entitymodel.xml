        <field name="paintBrand" type="id-long"></field>
        <field name="paintType" type="id-long"></field>
        <field name="paintName" type="id-long"></field>
        <field name="qtyUnit" type="id-long"></field>
        <field name="orderQty" type="fixed-point"></field>
        <field name="orderQtyLB" type="fixed-point"></field>
        <field name="producedQty" type="fixed-point"></field>
        <field name="itemLength" type="fixed-point"></field>
