        <field name="supplierNm" type="id-long"></field>
        <field name="agentId" type="id-long"></field>
        <field name="poStatus" type="id-long"></field>
        <field name="priceTerm" type="id-long"></field>
        <field name="freightTerm" type="id-long"></field>
