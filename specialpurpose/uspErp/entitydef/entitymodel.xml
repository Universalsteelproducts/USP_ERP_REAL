        <alias entity-alias="PI" field="invoicedQtySubTotal" name="invoicedQtySubTotal"/>
        <alias entity-alias="PS" field="shipmentQtySubTotal" name="shipmentQtySubTotal"/>

        <view-link entity-alias="PQUP" rel-entity-alias="PI" rel-optional="true">
            <key-map field-name="poNo"/>
            <key-map field-name="voidYn"/>
            <key-map field-name="lotNo"/>
