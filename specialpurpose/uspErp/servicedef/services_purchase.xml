<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>UspErp Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="RUPoList" default-entity-name="PoMaster" engine="java" auth="true"
             location="services.PurchaseServices" invoke="RUPoList">
        <attribute name="crudMode" mode="IN" type="String" optional="false"/>
        <attribute name="schPoStatus" mode="IN" type="String" optional="true"/>
        <attribute name="schSupplierId" mode="IN" type="String" optional="true"/>
        <attribute name="schPoNo" mode="IN" type="String" optional="true"/>
        <attribute name="schProductId" mode="IN" type="String" optional="true"/>
        <attribute name="schCustomerId" mode="IN" type="String" optional="true"/>
        <attribute name="schOrderFromDate" mode="IN" type="Timestamp" optional="true"/>
        <attribute name="schOrderToDate" mode="IN" type="Timestamp" optional="true"/>
        <attribute name="data" mode="OUT" type="java.util.List" optional="true"/>
        <attribute name="reqData" mode="IN" type="String" optional="true"/>
        <attribute name="draw" mode="INOUT" type="String" optional="true"/>
        <attribute name="recordsTotal" mode="OUT" type="Integer" optional="true"/>
        <attribute name="recordsFiltered" mod e="OUT" type="Integer" optional="true"/>
    </service>

    <service name="CRUPoList" default-entity-name="PoMaster" engine="java" auth="true"
             location="services.PurchaseServices" invoke="CRUPoList">
        <attribute name="crudMode" mode="IN" type="String" optional="false"/>
        <auto-attributes include="pk" mode="IN" optional="true"/>
        <auto-attributes include="nonpk" mode="IN" optional="true"/>
        <attribute name="poNo" mode="IN" type="String" optional="true"/>
        <attribute name="data" mode="OUT" type="java.util.List" optional="true"/>
        <attribute name="reqData" mode="IN" type="String" optional="true"/>
        <attribute name="recordsTotal" mode="OUT" type="Integer" optional="true"/>
        <attribute name="recordsFiltered" mode="OUT" type="Integer" optional="true"/>
        <attribute name="successStr" mode="OUT" type="String" optional="true"/>
    </service>

</services>